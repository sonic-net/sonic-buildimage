[Unit]
Description=Accton AS7726-32X Platform initialization service
Before=pmon.service determine-reboot-cause.service system-health.service
After=as7726-32x-platform-handle_mac.service
DefaultDependencies=no

[Service]
Type=oneshot
RemainAfterExit=yes
ExecStart=/usr/local/bin/accton_as7726_32x_util.py install
ExecStop=/usr/local/bin/accton_as7726_32x_util.py clean

# Resource Limitations
LimitCORE=infinity

[Install]
WantedBy=multi-user.target