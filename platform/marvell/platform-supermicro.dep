MPATH       := $($(SUPERMICRO_SSE_G3748_PLATFORM)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) $(PLATFORM_PATH)/platform-supermicro.dep $(PLATFORM_PATH)/platform-supermicro.mk
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(MPATH)/,$(shell cd $(MPATH) && git ls-files))

$(SUPERMICRO_SSE_G3748_PLATFORM)_CACHE_MODE  := GIT_CONTENT_SHA
$(SUPERMICRO_SSE_G3748_PLATFORM)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SUPERMICRO_SSE_G3748_PLATFORM)_DEP_FILES   := $(DEP_FILES)
$(SUPERMICRO_SSE_G3748_PLATFORM)_SMDEP_FILES := $(SMDEP_FILES)
