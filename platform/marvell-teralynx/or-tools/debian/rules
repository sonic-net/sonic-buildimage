#!/usr/bin/make -f

# OR-Tools debian package rules
# All build logic is handled by the main Makefile

%:
	dh $@

override_dh_auto_configure:
	# Configuration handled by Makefile

override_dh_auto_build:
	# Build handled by Makefile  
	$(MAKE) library

override_dh_auto_install:
	# Install handled by dh_install

override_dh_auto_clean:
	# Clean handled by Makefile
	$(MAKE) clean

override_dh_auto_test:
	# Skip tests
