
SPATH       := $($(STP)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/sonic-stp.mk rules/sonic-stp.dep
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(STP)_CACHE_MODE  := GIT_CONTENT_SHA
$(STP)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(STP)_DEP_FILES   := $(DEP_FILES)
$(STP)_SMDEP_FILES := $(SMDEP_FILES)
$(STP)_SMDEP_PATHS := $(SPATH)
