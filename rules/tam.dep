
SPATH       := $($(SONIC_TAM)_SRC_PATH)
DEP_FILES   := $(SONIC_COMMON_FILES_LIST) rules/tam.mk rules/tam.dep   
DEP_FILES   += $(SONIC_COMMON_BASE_FILES_LIST)
SMDEP_FILES := $(addprefix $(SPATH)/,$(shell cd $(SPATH) && git ls-files))

$(SONIC_TAM)_CACHE_MODE  := GIT_CONTENT_SHA 
$(SONIC_TAM)_DEP_FLAGS   := $(SONIC_COMMON_FLAGS_LIST)
$(SONIC_TAM)_DEP_FILES   := $(DEP_FILES)
$(SONIC_TAM)_SMDEP_FILES := $(SMDEP_FILES)
$(SONIC_TAM)_SMDEP_PATHS := $(SPATH)

